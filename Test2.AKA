test2 {

	def increment(number oldNum : number newNum) {
		newNum = oldNum + 1;
	}
	
	main {
		number i = 0;
		
		while (i <= 10) 
		{
			if (i < 3) {
				display("i < 3");
			} elseif (i < 5) {
				display("i < 6");
			} elseif (i < 8) {
				display("i < 8");
			} else {
				display("i <= 10");
			}
			i = increment(i);
		}
	}

}

